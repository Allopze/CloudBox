{
  "name": "cloudbox-backend",
  "version": "1.0.0",
  "type": "module",
  "description": "CloudBox - Cloud Storage Backend",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch --ignore './data/**' --ignore './prisma/**' src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
